 friends can take it at: <a href={url}>{url}</a></h6>\n\n            <CopyToClipboard text={url}\n                onCopy={() => setCopied(true)}>\n                <button className=\"button is-light with-space-on-right\">Copy Link to Clipboard</button>\n            </CopyToClipboard>\n\n            <div>\n                <br />\n                {addQuestions && <button onClick={() => addQuestions(quiz)} className=\"button is-info with-space-on-right\">Add More Questions</button>}\n                {deleteQuiz && <button onClick={() => deleteQuiz(quiz)} className=\"button is-danger\">Delete Quiz</button>}\n            </div>\n\n            {copied && \n                <Checkmark size={40} />\n            }\n        </div>\n    )\n};\n \nexport default QuizPreview;","import React, { useState, useEffect } from 'react';\nimport getTopics from 'data/getTopics';\nimport { Typeahead } from 'react-bootstrap-typeahead'; \n\n\nconst Filter = ({ filterByTopic }) => {\n\n    const [topic, setTopic] = useState([\"\"]);\n\n    useEffect(() => {\n        if(topic.length === 0) \n            return filterByTopic(\"\");\n        \n        const chosenTopic = topic[0] && topic[0].trim();\n\n        if(chosenTopic)\n            filterByTopic(chosenTopic);\n\n    }, [topic]);\n\n    return(\n        <Typeahead\n            id={1}\n            onChange={topic => setTopic(topic)}\n            options={getTopics()}\n            selected={topic}\n        />\n    )\n}\n \nexport default Filter;","import React from 'react';\nimport Spinner from 'react-bootstrap/Spinner';\n\nconst Loading = () => (\n    <Spinner variant=\"primary\" animation=\"border\" role=\"status\">\n        <span className=\"sr-only\">Loading...</span>\n    </Spinner>\n)\n \nexport default Loading;","import React from 'react';\nimport QuizPreview from 'components/portal/quizPreview';\nimport { webHost } from 'config';\nimport Filter from './filter';\nimport Loading from '../loading';\n\nconst Quizzes = ({ quizzes, filterBy